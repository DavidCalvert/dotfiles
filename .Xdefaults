*title: Terminal
*loginShell: false
*geometry: 240x40
*saveLines: 10000
*cutchars: "`\"'&()*,:;<=>?[\\]^{|}$"
*font: xft:Sauce Code Powerline:size=13
*boldFont: xft:Sauce Code Powerline:size=13
*italicFont: xft:Sauce Code Powerline:size=13
*bolditalicFont: xft:Sauce Code Powerline:size=13
*scrollBar: false
*visualBell: true
*letterSpace: -1
*cursorBlink: true
*cursorUnderline: 1
*foreground: rgb:a8/a8/a8
*background: #000C0F
*color0: rgb:00/00/00
*color1: rgb:a8/00/00
*color2: rgb:00/a8/00
*color3: rgb:a8/54/00
*color4: rgb:00/00/a
*color5: rgb:a8/00/a8
*color6: rgb:00/a8/a8
*color7: rgb:a8/a8/a8
*color8: rgb:54/54/54
*color9: rgb:fc/54/54
*color10: rgb:54/fc/54
*color11: rgb:fc/fc/54
*color12: rgb:54/54/fc
*color13: rgb:fc/54/fc
*color14: rgb:54/fc/fc
*color15: rgb:fc/fc/fc
*colorBD: rgb:fc/fc/fc
